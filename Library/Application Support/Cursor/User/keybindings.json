// Place your key bindings in this file to override the defaults
[
  // Movements
  {
    "command": "composerMode.agent",
    "key": "cmd+i"
  },
  {
    "command": "workbench.action.navigateLeft",
    "key": "ctrl+h"
  },
  {
    "command": "workbench.action.navigateUp",
    "key": "ctrl+j"
  },
  {
    "command": "workbench.action.navigateDown",
    "key": "ctrl+k"
  },
  {
    "command": "workbench.action.navigateRight",
    "key": "ctrl+l"
  },
  {
    "command": "omn.resumeLastCommand",
    "key": "space space",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  // from neovim
  {
    "command": "editor.action.rename",
    "key": "space r n",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  // Search for things
  {
    "command": "workbench.action.quickOpen",
    "key": "space ;",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "search.action.openEditor",
    "key": "space '",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "key": "space /",
    "command": "fuzzySearch.activeTextEditor",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "workbench.action.gotoSymbol",
    "key": "space s d",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "key": "space s w",
    "command": "omn.ripgrepSearch",
    "when": "(vim.mode == 'Normal' && editorTextFocus) || (vim.mode == 'View' && editorTextFocus)"
  },
  {
    // space w -> save file
    "key": "space w",
    "command": "workbench.action.files.save",
    "when": "vim.mode == 'Normal' && editorTextFocus && !inputFocus"
  },
  {
    // space q -> close tab/close item
    "key": "space q",
    "command": "workbench.action.closeActiveEditor",
    "when": "vim.mode == 'Normal' && editorTextFocus && !inputFocus"
  },
  {
    // space [ -> go back
    "key": "space [",
    "command": "workbench.action.navigateBack",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },

  {
    // space ] -> go forwards
    "key": "space ]",
    "command": "workbench.action.navigateForward",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },

  // Accept suggestion with Ctrl+E
  {
    "key": "ctrl+e",
    "command": "acceptSelectedSuggestion",
    "when": "suggestWidgetVisible"
  },
  // Trigger autocomplete with Ctrl+X
  {
    "key": "ctrl+x",
    "command": "editor.action.triggerSuggest",
    "when": "editorTextFocus"
  },
  {
    "args": {
      "commands": ["workbench.files.action.focusFilesExplorer"]
    },
    "command": "runCommands",
    "key": "space e",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  // Coding
  {
    "command": "editor.action.codeAction",
    "key": "space i",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "editor.action.showHover",
    "key": "shift-k",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },

  {
    "command": "workbench.action.closeActiveEditor",
    "key": "space b d",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "workbench.action.closeOtherEditors",
    "key": "space b o",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "lazygit-vscode.toggle",
    "key": "space g l",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "editor.action.addSelectionToNextFindMatch",
    "key": "ctrl-n",
    "when": "vim.mode == 'Normal' || vim.mode == 'Visual' && editorTextFocus"
  },
  // LSP
  {
    "command": "editor.action.goToReferences",
    "key": "g r",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "omn.goToReferences",
    "key": "g o r",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "editor.action.revealDefinition",
    "key": "g d",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "omn.goToDefinition",
    "key": "g o d",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "editor.action.goToTypeDefinition",
    "key": "g t",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "omn.goToTypeDefinition",
    "key": "g o t",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "editor.action.goToImplementation",
    "key": "g i",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "omn.goToImplementation",
    "key": "g o i",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  // File Explorer
  {
    "command": "renameFile",
    "key": "r",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "command": "filesExplorer.copy",
    "key": "c",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "command": "filesExplorer.paste",
    "key": "p",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "command": "filesExplorer.cut",
    "key": "x",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "command": "deleteFile",
    "key": "d",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "command": "explorer.newFile",
    "key": "a",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "command": "explorer.openToSide",
    "key": "s",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "args": {
      "commands": [
        "workbench.action.splitEditorDown",
        "explorer.openAndPassFocus",
        "workbench.action.closeOtherEditors"
      ]
    },
    "command": "runCommands",
    "key": "shift-s",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "command": "explorer.openAndPassFocus",
    "key": "enter",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceIsFolder && !inputFocus"
  },
  {
    "command": "list.toggleExpand",
    "key": "enter",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && explorerResourceIsFolder && !inputFocus"
  },
  // Debug
  {
    "command": "workbench.action.debug.selectandstart",
    "key": "space d a",
    "when": "vim.mode == 'Normal' && editorTextFocus && debuggersAvailable"
  },
  {
    "command": "workbench.action.debug.stop",
    "key": "space d t",
    "when": "vim.mode == 'Normal' && editorTextFocus && inDebugMode && !focusedSessionIsAttached"
  },
  {
    "command": "workbench.action.debug.stepOver",
    "key": "space d o",
    "when": "vim.mode == 'Normal' && editorTextFocus && inDebugMode && debugState == 'stopped'"
  },
  {
    "command": "editor.debug.action.toggleBreakpoint",
    "key": "space d b",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "editor.debug.action.showDebugHover",
    "key": "space d e",
    "when": "vim.mode == 'Normal' && editorTextFocus && inDebugMode && debugState == 'stopped'"
  },
  {
    "command": "workbench.action.debug.continue",
    "key": "space d c",
    "when": "vim.mode == 'Normal' && editorTextFocus && inDebugMode && debugState == 'stopped'"
  },
  // Error & Diagnostic Navigation
  {
    "command": "editor.action.marker.next",
    "key": "] e",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "editor.action.marker.prev",
    "key": "[ e",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "editor.action.marker.nextInFiles",
    "key": "] d",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "editor.action.marker.prevInFiles",
    "key": "[ d",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "cursorTop",
    "key": "g g",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "testing.runAtCursor",
    "key": "space t r",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "testing.reRunLastRun",
    "key": "space t l",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "-testing.runCurrentFile",
    "key": "space t f",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "workbench.view.testing.focus",
    "key": "space t s",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  // Harpoon
  {
    "command": "vscode-harpoon.addEditor",
    "key": "space m",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "vscode-harpoon.editEditors",
    "key": "space k",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "vscode-harpoon.gotoEditor1",
    "key": "space 1",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "vscode-harpoon.gotoEditor2",
    "key": "space 2",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "vscode-harpoon.gotoEditor3",
    "key": "space 3",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "vscode-harpoon.gotoEditor4",
    "key": "space 4",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "vscode-harpoon.gotoEditor5",
    "key": "space 5",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  // olly extensions
  {
    "command": "omn.swapToSibling",
    "key": "space g s",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "omn.searchSymbols",
    "args": "react",
    "key": "space s r",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "omn.searchSymbols",
    "args": "all",
    "key": "space s a",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "omn.searchSymbols",
    "args": "zod",
    "key": "space s z",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "omn.searchSymbols",
    "args": "class",
    "key": "space s c",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "omn.searchSymbols",
    "args": "method",
    "key": "space s m",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "omn.searchSymbols",
    "args": "function",
    "key": "space s f",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "omn.searchSymbols",
    "args": "variable",
    "key": "space s v",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "workbench.action.terminal.toggleTerminal",
    "key": "cmd+,"
  },
  {
    "command": "workbench.action.toggleMaximizedPanel",
    "key": "cmd+shift+,"
  }
]
