{
  "window.commandCenter": 1,
  "git.enableSmartCommit": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "workbench.colorTheme": "Catppuccin Mocha",
  "editor.formatOnSave": true,
  "editor.fontFamily": "JetBrainsMono Nerd Font, JetBrainsMono NF, monospace",
  // vim setup
  "vim.useSystemClipboard": true,
  "vim.foldfix": true,
  "workbench.preferredLightColorTheme": "Catppuccin Latte",
  "workbench.preferredDarkColorTheme": "Catppuccin Mocha",
  "window.systemColorTheme": "auto",
  "window.autoDetectColorScheme": true,
  "cursor.composer.shouldAllowCustomModes": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "lazygit-vscode.configPath": "~/.config/lazygit/config-light.yml",
  "gitlens.plusFeatures.enabled": false,
  "gitlens.views.commitDetails.files.layout": "tree",
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "editor.minimap.renderCharacters": false,
  "jest.runMode": "on-demand",
  "editor.inlayHints.enabled": "on",
  "editor.cursorSurroundingLines": 20,
  "editor.cursorSurroundingLinesStyle": "default",
  "breadcrumbs.enabled": true,
  "git-worktree-menu.worktreeDir": "~/projects/onsi-repo",
  "cursor.cpp.disabledLanguages": ["plaintext"],
  "cursor.cpp.enablePartialAccepts": true,
  "githubPullRequests.pullBranch": "never",
  "datadog.telemetry.setup.suppressMigrationNotification": true,
  "datadog.telemetry.setup.enableTelemetry": "disabled",
  "playwright.reuseBrowser": true,
  "playwright.showTrace": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.hideUnchangedRegions.enabled": true,
  "eslint.execArgv": ["--max_old_space_size=16000"],
  "typescript.tsserver.maxTsServerMemory": 16000,
  "workbench.editor.enablePreview": false,
  "excalidraw.theme": "light",
  "datadog.connection.notification.suppressSignInNotification": true,
  "datadog.connection.oauth.setup.domain": "app.datadoghq.eu",
  "remote.SSH.remotePlatform": {
    "ubuntu@n8n-dev": "linux"
  },
}
